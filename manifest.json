{
  "manifest_version": 2,
  "name": "Simple Extension",
  "version": "1.0",
  "description": "A simple Chrome extension with a popup",
  "permissions": ["activeTab", "https://api.openai.com/"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
